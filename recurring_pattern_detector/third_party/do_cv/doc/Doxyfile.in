PROJECT_NAME = "DO"


INPUT = @CMAKE_SOURCE_DIR@/src/DO/ \
        @CMAKE_SOURCE_DIR@/doc/MainPage.dox
RECURSIVE = YES
EXCLUDE_PATTERNS += *Match*
EXCLUDE_PATTERNS += *.cpp

GENERATE_XML = YES

GENERATE_TREEVIEW = YES
HTML_DYNAMIC_SECTIONS = YES
TAB_SIZE = 2
HIDE_SCOPE_NAMES = YES
ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
PREDEFINED = DOXYGEN_SHOULD_SKIP_THIS
USE_MATHJAX = YES
